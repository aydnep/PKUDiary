{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "globals": {
    "shallow": true,
    "render": true,
    "mount": true
  },
  "rules": {
    "array-bracket-spacing": [
      "error",
      "never"
    ],
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "arrow-spacing": [
      "error",
      {
        "after": true,
        "before": true
      }
    ],
    "block-spacing": [
      "error",
      "always"
    ],
    "brace-style": [
      "error",
      "1tbs"
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "comma-spacing": [
      "error",
      {
        "after": true,
        "before": false
      }
    ],
    "eol-last": [
      "error",
      "always"
    ],
    "func-names": 0,
    "func-call-spacing": [
      "error",
      "never"
    ],
    "indent": [
      "error",
      2
    ],
    "key-spacing": [
      2
    ],
    "keyword-spacing": [
      "error",
      {
        "after": true,
        "before": true
      }
    ],
    "no-debugger": 2,
    "no-multi-spaces": 2,
    "no-multiple-empty-lines": 2,
    "no-tabs": 2,
    "no-trailing-spaces": 2,
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "space-infix-ops": 2,
    "quotes": [
      "error",
      "single"
    ],
    "space-before-blocks": [
      "error"
    ],
    "space-before-function-paren": [
      "error",
      "never"
    ],
    "space-in-parens": [
      "error",
      "never"
    ],
    "spaced-comment": [
      "error",
      "always"
    ],
    "import/no-unresolved": [
      2,
      {
        "commonjs": true,
        "amd": true
      }
    ],
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "components": [
          "Link",
          "a"
        ],
        "specialLink": [
          "to",
          "hrefLeft",
          "hrefRight",
          "onClick"
        ],
        "aspects": [
          "noHref",
          "invalidHref",
          "preferButton"
        ]
      }
    ],
    "react/forbid-prop-types": [
      "error",
      {
        "forbid": [
          "any"
        ]
      }
    ],
    "react/require-default-props": 0,
    "react/jsx-filename-extension": 0,
    "react/no-string-refs": 2,
    "jsx-a11y/href-no-hash": 0,
    "jsx-a11y/no-noninteractive-element-interactions": 0,
    "class-methods-use-this": 1,
    "jsx-a11y/click-events-have-key-events": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "camelcase": [0, {"properties": "never"}],
    "jsx-a11y/mouse-events-have-key-events": 0,
    "react/no-array-index-key": 0,
    "max-len": ["error", { "comments": 200, "code": 120 }],
    "react/style-prop-object": 0
  }

}